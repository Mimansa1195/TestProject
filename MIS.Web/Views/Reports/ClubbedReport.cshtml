<div class="container-fluid">
    <section class="card">
        <div class="card-block">
            <h5 class="with-border m-t-0">Clubbed Reports</h5>
            <div class="text-right" style="padding-bottom:5px" id="divDeligation">

            </div>
            <div class="row form-body" id="reportFilter">
                <div class="form-group col-md-3">
                    <label class="control-label">From Date<span class="spnError">*</span></label>
                    <div>
                        <div class="inputGroupContainer date date-picker" id="fromDateEmp">
                            <div class="input-group margin-top-8">
                                <input readonly="" class="form-control" name="fromDateEmp" placeholder="From Date" type="text" />
                                <span class=" input-group-btn">
                                    <button class="btn default" type="button"><i class="fa fa-calendar"></i></button>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label">Till Date<span class="spnError">*</span></label>
                    <div>
                        <div class="inputGroupContainer date date-picker" id="tillDateEmp">
                            <div class="input-group margin-top-8">
                                <input readonly="" class="form-control " name="tillDateEmp" placeholder="Till Date" type="text" />
                                <span class=" input-group-btn">
                                    <button class="btn default" type="button"><i class="fa fa-calendar"></i></button>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label">Department Name<span class="spnError">*</span></label>
                    <div>
                        <p class="form-control-static">
                            <select class="form-control" multiple="multiple" id="department"></select>
                        </p>
                    </div>
                </div>

                <div class="form-group col-md-3">
                    <label class="control-label">Team Name<span class="spnError">*</span></label>
                    <div>
                        <p class="form-control-static">
                            <select class="form-control" multiple="multiple"  id="team"></select>
                        </p>
                    </div>
                </div>
                @*<div class="clearfix"></div>*@
                <div class="form-group col-md-3">
                    <label class="control-label">Location<span class="spnError">*</span></label>
                    <div>
                        <p class="form-control-static">
                            <select class="form-control" multiple="multiple" id="location"></select>
                        </p>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label">Status<span class="spnError">*</span></label>
                    <div>
                        <p class="form-control-static">
                            <select class="form-control" id="status">
                                <option value="" selected>All</option>
                                <option value="true">Active</option>
                                <option value="false">Inactive</option>
                            </select>
                        </p>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <div>
                        <button id="btnProcessReportEmp" type="button" class="btn green search-margin-top-30" onclick="processClubbedReport()"><i class="fa fa-search"></i>&nbsp;Search</button>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 clearfix table-responsive">
                    <table id="tblClubbedReportList" class="table table-bordered table-hover display table-striped" role="grid" aria-describedby="example1-info"></table>
                </div>
            </div>
        </div>
    </section>
</div>

<div id="mypopupShowDetailsModal" class="modal fade" role="dialog" aria-labelledby="mypopupShowDetailsModal" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 id="modalTitle" class="modal-title">Show Details</h4>
            </div>
            <div class="modal-body" id="popupDataShow">

            </div>
            <div class="modal-footer">
                <button id="btnClosePopUp" type="button" class="btn btn-default"><i class="fa fa-times"></i>&nbsp;Close </button>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/app/Reports/clubbedreport.js?v=3.7"></script>