<div class="container-fluid">
    <section class="card">
        <div class="card-block">
            <h5 class="with-border m-t-0">Team Forms</h5>
            <div role="tabpanel" class="tab-pane">
                <div class="row">
                    <div class="col-md-2">
                        <label>Form Type</label>
                    </div>
                    <div class="col-md-4">
                        <select id="departmentalForm" class="form-control select2 validation-required" placeholder="Form"></select>
                    </div>
                    <div class="col-md-2">
                        @*<button type="button" class="btn btn-success" id="downloadAllForm">Download All Forms</button>*@
                    </div>
                </div>
                <div class="row form-body">
                    <div class="col-md-12 clearfix">
                        <table id="tblTeamForms" class="table table-bordered table-hover display table-striped" role="grid" aria-describedby="example1-info"></table>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>

<div id="viewDocumentModal" class="modal fade" role="dialog" aria-labelledby="viewDocumentModal" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 id="modalTitle" class="modal-title">View Document</h4>
            </div>
            <div class="container margin-top-10">
                <div id="popupWindowViewDocument">
                    <object id="objViewPdf" type="application/pdf" data="" width="100%" height="500" internalinstanceid="65">No Support</object>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/app/Form/teamForm.js?v=1"></script>