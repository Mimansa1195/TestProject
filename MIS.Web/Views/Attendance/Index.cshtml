<div class="container-fluid">
    <section class="card">
        <div class="card-block">
            <h5 class="with-border m-t-0">Attendance Register</h5>
            <div class="text-right" style="padding-bottom:5px" id="divDeligation">

            </div>
            <div class="row">
                <div class="col-xl-12 col-lg-12">
                    <section class="tabs-section">
                        <div class="tabs-section-nav tabs-section-nav-left">
                            <ul class="nav" role="tablist">
                                <li class="nav-item" id="tabEmployeeAttendance">
                                    <a class="nav-link active" href="#tabs-2-tab-1" role="tab" data-toggle="tab">
                                        <span class="nav-link-in">Employee Attendance</span>
                                    </a>
                                </li>
                                <li class="nav-item" id="tabStaffAttendance">
                                    <a class="nav-link" href="#tabs-2-tab-2" role="tab" data-toggle="tab">
                                        <span class="nav-link-in">Staff Attendance</span>
                                    </a>
                                </li>
                            </ul>
                        </div><!--.tabs-section-nav-->

                        <div class="tab-content no-styled profile-tabs">
                            <div role="tabpanel" class="tab-pane active" id="tabs-2-tab-1">
                                <section class="box-typical box-typical-padding">
                                    <div class="row form-body">
                                        <div class="form-group col-md-4">
                                            <label>From Date<span class="spnError">*</span></label>
                                            @*<input readonly="" class="form-control validation-required" id="fromDateEmp" placeholder="From Date" type="text" />*@
                                            <div class="inputGroupContainer date date-picker">
                                                <div class="input-group">
                                                    <input readonly="" class="form-control validation-required" id="fromDateEmp" placeholder="From Date" type="text" />
                                                    <span class=" input-group-btn">
                                                        <button class="btn default" type="button"><i class="fa fa-calendar"></i></button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label>Till Date<span class="spnError">*</span></label>
                                            <div class="inputGroupContainer date date-picker">
                                                <div class="input-group">
                                                    <input readonly="" class="form-control validation-required" id="tillDateEmp" placeholder="From Date" type="text" />
                                                    <span class=" input-group-btn">
                                                        <button class="btn default" type="button"><i class="fa fa-calendar"></i></button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label>Department Name<span class="spnError">*</span></label>
                                            <select class="form-control" id="department" multiple="multiple" onchange="getReportingManagersByDepartmentId();"></select>
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label>Reporting  Manager<span class="spnError">*</span></label>
                                            <select class="form-control" id="reportingManager" multiple="multiple" onchange="getEmployeesAccordingToSelection();"></select>
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label>Employee Code<span class="spnError">*</span></label>
                                            <select class="form-control" multiple="multiple" id="employeeCode"></select>
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label>Employee Name<span class="spnError">*</span></label>
                                            <select class="form-control" multiple="multiple" id="employeeName"></select>
                                        </div>
                                        <div class="form-group col-md-12">
                                            <div class="form-group text-right">
                                                <button id="btnProcessReportEmp" type="button" class="btn green" onclick="processAttendanceSummaryReport()"><i class="fa fa-search"></i>&nbsp;Search</button>
                                            </div>
                                        </div>
                                        <div class="col-md-12 clearfix">
                                            <table id="tblEmployeeAttendanceList" class="table table-bordered table-hover display table-striped" role="grid" aria-describedby="example1-info"></table>
                                        </div>
                                    </div>
                                </section>
                            </div><!--.tab-pane-->
                            <div role="tabpanel" class="tab-pane" id="tabs-2-tab-2">
                                <section class="box-typical box-typical-padding">
                                    <div class="row form-body">
                                        <div class="rightMainDivContainer">
                                            <div id="ChahgeContactDetails-container">
                                                <div class="form-group col-md-4">
                                                    <label>From Date<span class="spnError">*</span></label>
                                                    <div class="inputGroupContainer date date-picker">
                                                        <div class="input-group">
                                                            <input readonly="" class="form-control validation-required" id="fromDate" placeholder="From Date" type="text" />
                                                            <span class=" input-group-btn">
                                                                <button class="btn default" type="button"><i class="fa fa-calendar"></i></button>
                                                            </span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group col-md-4">
                                                    <label>Till Date<span class="spnError">*</span></label>
                                                    <div class="inputGroupContainer date date-picker">
                                                        <div class="input-group">
                                                            <input readonly="" class="form-control validation-required" id="tillDate" placeholder="From Date" type="text" />
                                                            <span class=" input-group-btn">
                                                                <button class="btn default" type="button"><i class="fa fa-calendar"></i></button>
                                                            </span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group col-md-4">
                                                    <label>Employee Name<span class="spnError">*</span></label>
                                                    <select class="form-control" id="StaffName"></select>
                                                </div>
                                                <div class="form-group col-md-12">
                                                    <div class="form-group text-right">
                                                        <button id="btnProcessReport" type="button" class="btn green" onclick="getAttendanceRegisterForSupportingStaffMembers()"><i class="fa fa-search"></i>&nbsp;Search</button>
                                                    </div>
                                                </div>
                                                <div class="clearBoth"></div>
                                            </div>
                                        </div>
                                        <div class="col-md-12 clearfix">
                                            <table id="tblStaffAttendanceList" class="table table-bordered table-hover display table-striped" role="grid" aria-describedby="example1-info"></table>
                                        </div>
                                    </div>
                                </section>
                            </div><!--.tab-pane-->
                        </div><!--.tab-content-->
                    </section><!--.tabs-section-->
                </div>
            </div>
        </div>
    </section>
</div>
<script src="~/Scripts/app/Attendance/staffattendance.js?v=2"></script>
<script src="~/Scripts/app/Attendance/employeeattendance.js?v=4.5"></script>