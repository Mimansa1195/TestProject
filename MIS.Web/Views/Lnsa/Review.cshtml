                    </section>
                    <input type="hidden" id="hiddenTempApprovedShiftId" value="" />
                </div>
                <div class="modal-footer">
                    <button id="btnLnsaApprovedClose" type="button" class="btn btn-default"><i class="fa fa-times"></i>&nbsp;Close </button>
                </div>
            </div>
        </div>
    </div>
</div>

