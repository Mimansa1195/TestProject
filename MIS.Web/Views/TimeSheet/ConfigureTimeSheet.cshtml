<div class="container-fluid">
    <section class="card">
        <div class="card-block">
            <h5 class="with-border m-t-0" id="changeHeading">Manage Projects</h5>
            <div class="row" id="divLeave">
                <div class="col-xl-12 col-lg-12">
                    <section class="tabs-section">
                        <div class="tabs-section-nav tabs-section-nav-left">
                            <ul class="nav" role="tablist">
                                <li class="nav-item">
                                    <a class="nav-link active" href="#tabAssignProjects" role="tab" data-toggle="tab">
                                        <span class="nav-link-in text-green">Assigned Projects</span>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="#tabMarkSubscription" role="tab" style="display:none" id="hideTab" data-toggle="tab">
                                        <span class="nav-link-in text-green">Timesheet Subscription</span>
                                    </a>
                                </li>
                            </ul>
                        </div><!--.tabs-section-nav-->
                        <div class="tab-content no-styled profile-tabs">
                            <div role="tabpanel" class="tab-pane active" id="tabAssignProjects">
                                <section class="box-typical box-typical-padding">
                                    <div class="row form-body">
                                        <div class="col-md-12 clearfix">
                                            <div class="displayDiv">
                                                <div class="form-group col-md-12">
                                                    <table id="tblProjectList" data-filter-control="true" data-toolbar="#toolbar" class="table table-hover table-bordered display table-striped"></table>
                                                    <div style="text-align: center">
                                                        <input type="button" class="btn generalButton" value="Save" id="btnSavePrefrences" style="font-size: 20px; height: 40px; width: 200px;" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </section>
                            </div><!--.tab-pane-->
                            <div role="tabpanel" class="tab-pane" id="tabMarkSubscription">
                                <section class="box-typical box-typical-padding">
                                    <div class="row form-body">
                                        <div class="col-md-12 clearfix">
                                            <div class="displayDiv">
                                                <div class="form-group row">
                                                    <div class="col-md-5 form-group">
                                                        <h4>Ineligible For Timesheets</h4>
                                                        <select id="lstAllExcludedUsers" size="10" style="width: 100%; height: 300px; overflow:auto" class="form-control formddl generalinput"></select>
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <div class="col-md-12" style="margin-top: 150px;">
                                                            <button id="btnAddUser" type="button" class="btn default" data-toggle="tooltip" title="Make user eligible">>>></button>
                                                            <button id="btnRemoveUser" type="button" class="btn default margin-top-10" data-toggle="tooltip" title="Make user ineligible"><<<</button>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-5 form-group">
                                                        <div class="">
                                                            <h4>Eligible For Timesheets</h4>
                                                            <select id="lstAllEligibleUsers" size="10" style="width: 100%; height: 300px; overflow:auto" class="form-control formddl generalinput"></select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </section>
                            </div><!--.tab-pane-->
                        </div>
                    </section>
                </div>
            </div>
        </div>
    </section>
</div>
<script src="~/Scripts/app/timesheet/ConfigureTimeSheet.js?v=2.5"></script>

