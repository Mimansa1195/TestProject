<div class="container-fluid">
    <section class="card">
        <div class="card-block">
            <h5 class="with-border m-t-0">User Access Card Mapping</h5>
            <div class="row">
                <div class="col-xl-12 col-lg-12">
                    <section class="tabs-section">
                        <div class="tabs-section-nav tabs-section-nav-left">
                            <ul class="nav" role="tablist">
                                <li class="nav-item" onclick="loadUserCardMappingGrid()">
                                    <a class="nav-link active" href="#tabMappedCard" role="tab" data-toggle="tab">
                                        <span class="nav-link-in text-green">Mapped Card</span>
                                    </a>
                                </li>

                                <li class="nav-item" onclick="loadUserUnMappedCardHistoryGrid()">
                                    <a class="nav-link" href="#tabUnMappedCardHistory" role="tab" data-toggle="tab">
                                        <span class="nav-link-in text-green">Unmapped Card History</span>
                                    </a>
                                </li>
                            </ul>
                        </div><!--.tabs-section-nav-->
                        <div class="tab-content no-styled profile-tabs">
                            <div role="tabpanel" class="tab-pane active" id="tabMappedCard">
                                <section class="box-typical box-typical-padding">
                                    <div class="row form-body">
                                        <div class="col-md-12 clearfix">
                                            <div>
                                                <button type="button" class="btn btn-success" onclick="addNewMapping()">Add new user card mapping</button>
                                            </div>
                                            <div role="tabpanel" class="tab-pane margin-top-10">
                                                <section>
                                                    <div class="row form-body">
                                                        <div class="col-md-12 clearfix">
                                                            <table id="tblUserCardMapping" class="table table-bordered table-hover display table-striped" role="grid"></table>
                                                        </div>
                                                    </div>
                                                </section>
                                            </div>

                                        </div>
                                    </div>
                                </section>
                            </div>
                            <div role="tabpanel" class="tab-pane" id="tabUnMappedCardHistory">
                                <section class="box-typical box-typical-padding">
                                    <div class="row form-body">
                                        <div class="col-md-12 clearfix">
                                            <div role="tabpanel" class="tab-pane margin-top-10">
                                                <section>
                                                    <div class="row form-body">
                                                        <div class="col-md-12 clearfix">
                                                            <table id="tblUnMappedCardHistory" class="table table-bordered table-hover display table-striped" role="grid"></table>
                                                        </div>
                                                    </div>
                                                </section>
                                            </div>

                                        </div>
                                    </div>
                                </section>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
        </div>
    </section>
</div>


<div id="modal-addUserCardMapping" class="modal fade" role="dialog" aria-labelledby="myDocumentsDetails" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 id="userCardMappingModalTitle" class="modal-title"></h4>
            </div>
            <div class="container margin-top-10">
                <div class="row" id="btnSwitch">
                    <div class="col-md-12">
                        <div class="form-group">
                            User Type
                            <input type="checkbox" id="userType" name="userType" data-toggle="toggle" data-on="Employee" data-off="Staff" data-onstyle="success" data-offstyle="info" data-size="small" data-width="100">
                        </div>
                    </div>
                </div>
                <div id="employeeDetail">
                    <div class="form-group row">
                        <div class="form-group">
                            <div class="col-md-4">
                                <label id="lblUser">Employee<span class="spnError">*</span></label>
                            </div>
                            <div class="col-md-8">
                                <select type="text" class="form-control select2 validation-required" id="employee">
                                    <option value="0">Select</option>
                                </select>

                                <label><span id="editEmployee"></span></label>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="staffDetail">
                    <div class="form-group row">
                        <div class="form-group">
                            <div class="col-md-4">
                                <label id="lblstaff">Staff<span class="spnError">*</span></label>
                            </div>
                            <div class="col-md-8">
                                <select type="text" class="form-control select2 validation-required" id="staff">
                                    <option value="0">Select</option>
                                </select>

                                <label><span id="editEmployee"></span></label>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="form-group row" id="joingDiv">
                    <div class="form-group">
                        <div class="col-md-4">
                            <label>Joining Date</label>
                        </div> 
                        <div class="col-md-8">
                            <input class="form-control"type="text" disabled id="joiningDate" />
                        </div>
                    </div>
                </div>

                <div class="form-group row">
                    <div class="form-group">
                        <div class="col-md-4">
                            <label>Access card<span class="spnError">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <select type="text" class="form-control validation-required select2" id="accessCardNo">
                                <option value="0">Select</option>
                            </select>
                        </div>
                    </div>
                </div>

                <div class="form-group row" id="assignedDate">
                    <div class="form-group">
                        <div class="col-md-4">
                            <label>Assigned From<span class="spnError">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <div class="inputGroupContainer date date-picker" id='fromPicker'>
                                <div class="input-group">
                                    <input type="text" class="form-control validation-required" id="txtFrom" readonly="" placeholder="Enter Assigned From">
                                    <span class=" input-group-btn">
                                        <button class="btn default" type="button"><i class="fa fa-calendar"></i></button>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="form-group row" id="isPimco">
                    <div class="form-group">
                        <div class="col-md-4">
                            <label>For Pimco use</label>
                        </div>
                        <div class="col-md-8">
                            <input type="checkbox" id="isPimcoUserCardMapping" onclick="changeUserCardMappingPreference()" />
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal-footer">
                <button id="addUserAccessCardbtn" type="button" class="btn btn-success" onclick="finalAddUserCardMapping();"><i class="fa fa-save"></i>&nbsp;Add</button>
                <button id="updateUserAccessCardbtn" type="button" class="btn btn-success" onclick="updateUserCardMapping();"><i class="fa fa-save"></i>&nbsp;Update</button>
                <button id="btnClose" type="button" class="btn btn-default"><i class="fa fa-times"></i>&nbsp;Close </button>
            </div>
        </div>
    </div>
</div>
<div id="deleteModal" class="modal fade in" role="dialog" aria-labelledby="deleteModal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                <h4 id="modalTitle" class="modal-title"> Delete User Card Mapping</h4>
            </div>
            <div class="modal-body">
                <input type="hidden" name="hdnCardId" id="hdnCardId" value="" />

                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label">Employee Name</label>
                                <div class="input-group">
                                    <input type="text" class="form-control" id="txtEmpName" disabled="disabled" placeholder="Employee Name">
                                </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label">Assigned Till<span class="spnError">*</span></label>
                            <div class="inputGroupContainer date date-picker" id='datePicker'>
                                <div class="input-group">
                                    <input type="text" class="form-control validation-required" id="txtTillDate" readonly="" placeholder="Assigned Till Date">
                                    <span class=" input-group-btn">
                                        <button class="btn default" type="button"><i class="fa fa-calendar"></i></button>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 text-right">
                        <div class="form-group">
                            <button type="button" class="btn btn-danger" onclick="deleteUserCard()"><i class="fa fa-save"></i>&nbsp;Delete</button>
                            <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times"></i>&nbsp;Close </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/app/AccessCard/userCardMapping.js?v=1.6"></script>
